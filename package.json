    "bungie-api-ts": "^4.2.0",
    "cross-fetch": "^3.0.6",
    "fixjson": "^1.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^9.0.1",
    "json-diff": "^0.5.4",
    "prettier-plugin-organize-imports": "^1.1.1",
    "typescript": "^3.9.2",
